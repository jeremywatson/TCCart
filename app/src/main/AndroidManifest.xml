<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="edu.gatech.seclass.tccart">

    <application
        android:name=".TCCartApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".AddCustomerActivity">
        </activity>
        <activity android:name=".LookupCustomerActivity">
        </activity>
        <activity android:name=".AddCustomerSuccessActivity">
        </activity>
        <activity android:name=".CustomerActionsActivity">
        </activity>
        <activity android:name=".BuildTransactionActivity">
        </activity>
        <activity android:name=".CustomerInfoActivity">
        </activity>
        <activity android:name=".ChargeCreditCardActivity">
        </activity>
        <activity android:name=".ChargeSuccessActivity">
        </activity>
        <activity android:name=".CustomerTransactionsActivity">
        </activity>
    </application>

</manifest>